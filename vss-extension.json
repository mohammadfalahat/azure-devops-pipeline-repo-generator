{
  "manifestVersion": 1,
  "id": "pipeline-generator",
  "publisher": "localdev",
  "version": "0.1.8",
  "name": "Pipeline Generator",
  "description": "Add a Generate Pipeline button next to each branch to bootstrap pipeline repositories.",
  "public": false,
  "categories": ["Azure Pipelines", "Azure Repos"],
  "icons": {
    "default": "dist/images/icon.svg"
  },
  "files": [
    { "path": "dist", "addressable": true },
    { "path": "README.md" }
  ],
  "contributions": [
    {
      "id": "generate-pipeline-action",
      "type": "ms.vss-web.action",
      "targets": [
        "ms.vss-code-web.git-branches-menu",
        "ms.vss-code-web.branches-menu",
        "ms.vss-code-web.git-branches-tree-menu",
        "ms.vss-code-web.git-branches-toolbar-menu"
      ],
      "properties": {
        "text": "Generate pipeline",
        "title": "Generate pipeline for this branch",
        "icon": "dist/images/icon.svg",
        "uri": "dist/menu-action.html",
        "registeredObjectId": "generate-pipeline-action"
      }
    }
  ],
  "targets": [
    { "id": "Microsoft.VisualStudio.Services.Cloud" },
    { "id": "Microsoft.TeamFoundation.Server", "version": "[16.0,20.0)" }
  ],
  "scopes": ["vso.code", "vso.project", "vso.release", "vso.build"],
  "links": {
    "getstarted": {
      "uri": "https://learn.microsoft.com/azure/devops/extend/get-started/node"
    },
    "repository": {
      "uri": "https://azure.buluttakin.com/tfs/_git/pipeline-generator"
    },
    "support": {
      "uri": "https://azure.buluttakin.com/tfs/"
    }
  },
  "content": {
    "details": {
      "path": "README.md"
    }
  },
  "branding": {
    "color": "#0078D4",
    "theme": "dark"
  }
}
